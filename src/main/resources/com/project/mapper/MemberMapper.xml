<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.project.mapper.MemberMapper">
	<!-- 등록 처리 -->
	<insert id="register">
		INSERT INTO
		member(user_no, user_id, user_pw, user_name, job)
		VALUES(member_seq.NEXTVAL, #{userId}, #{userPw}, #{userName}, #{job})
	</insert>

	<!-- 그룹코드 목록 조회 -->
	<!-- 권한 생성 -->
	<insert id="createAuth">
		INSERT INTO member_auth(user_no, auth)
		VALUES(member_seq.CURRVAL, #{auth})
	</insert>

	<!-- 상세 조회 -->

	<!-- 삭제 처리 -->

	<!-- 수정 처리 -->

</mapper>




